    {
      "agent": {
        "metrics_collection_interval": 10,
        "logfile": "/opt/aws/amazon-cloudwatch-agent/logs/amazon-cloudwatch-agent.log"
      },
      "logs": {
        "logs_collected": {
          "files": {
            "collect_list": [
              {
                "file_path": "/opt/aws/amazon-cloudwatch-agent/logs/amazon-cloudwatch-agent.log",
                "log_group_class": "STANDARD",
                "log_group_name": "messages",
                "log_stream_name": "{hostname}",
                "retention_in_days": 3
              }
            ]
          }
        }
      },
      "metrics": {
        "namespace": "Test5",
        "metrics_collected": {
          "cpu": {
            "measurement": [
              "cpu_usage_idle",
              "cpu_usage_iowait"
            ],
            "metrics_collection_interval": 30,
            "resources": [
              "*"
            ],
            "totalcpu": true
          },
          "disk": {
            "measurement": [
              "used_percent"
            ],
            "metrics_collection_interval": 30,
            "resources": [
              "*"
            ]
          },
          "diskio": {
            "measurement": [
              "io_time",
              "write_bytes",
              "read_bytes",
              "writes",
              "reads"
            ],
            "metrics_collection_interval": 30,
            "resources": [
              "*"
            ]
          },
          "mem": {
            "measurement": [
              "mem_used_percent"
            ],
            "metrics_collection_interval": 30
          },
          "net": {
            "measurement": [
              "bytes_sent",
              "bytes_recv",
              "packets_sent",
              "packets_recv"
            ],
            "metrics_collection_interval": 30,
            "resources": [
              "*"
            ]
          },
          "swap": {
            "measurement": [
              "swap_used_percent"
            ],
            "metrics_collection_interval": 30
          }
        }
      }
    }